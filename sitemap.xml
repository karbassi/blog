<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="sitemap.xsl"?>
<urlset xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://www.sitemaps.org/schemas/sitemap/0.9 httsp://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="https://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>https://tech.karbassi.com/</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>0.6</priority>
    </url>

    {% for post in site.posts %}
    <url>
        <loc>https://tech.karbassi.com{{ post.id }}</loc>
        <lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>0.6</priority>
    </url>
    {% endfor %}
</urlset>