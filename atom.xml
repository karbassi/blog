---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Mind Mining Medium</title>
    <link href="http://tech.karbassi.com/atom.xml" rel="self"/>
    <link href="http://tech.karbassi.com/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://tech.karbassi.com/</id>
    <author>
        <name>Ali Karbassi</name>
        <email>ali@karbassi.com</email>
    </author>
    
    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://tech.karbassi.com{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://tech.karbassi.com{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>